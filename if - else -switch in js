let marks=90;
if(marks>=90)
  console.log('grade o');
else if(marks>=80)
   console.log('grade a+');
else  if(marks>=70)
   console.log('grade a');
else   
    console.log('60%')

let num=2;
switch(num){
    case 1:
        console.log('A');
        break;
    case 2:
        console.log('A++');
        break;
    case 3:
        console.log('B');
        break;
    case 4:
        console.log('D');   
        break;
    }



